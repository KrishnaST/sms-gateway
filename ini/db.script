CREATE TABLE dbo.BANK_CONFIG
	(
	BANK_ID VARCHAR (10) NOT NULL,
	PARAM   VARCHAR (30) NOT NULL,
	VALUE   VARCHAR (60) NULL,
	PRIMARY KEY (BANK_ID, PARAM)
	)
GO

CREATE TABLE dbo.SMS
	(
	ID         BIGINT IDENTITY NOT NULL,
	MOBILE     VARCHAR (13) NOT NULL,
	SMSTEXT    VARCHAR (500) NOT NULL,
	BANK_ID    VARCHAR (10) NOT NULL,
	CHANNEL_ID VARCHAR (10) NULL,
	CREATEDDT  DATETIME DEFAULT (getdate()) NULL,
	RESPONSE   VARCHAR (200) NULL,
	PRIMARY KEY (ID)
	)
GO

